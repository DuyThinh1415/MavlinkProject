            </options>
        </parameter>
        <parameter name="C_V_ICR" type="uint32" default="3">
            <description>EO AutoICR Mode</description>
            <options>
                <option name="Auto" value="2">
                    <exclusions>
                        <exclude>C_V_ICR_MAN</exclude>
                    </exclusions>
                </option>
                <option name="Manual" value="3">
                    <exclusions>
                        <exclude>C_V_ICR_THR</exclude>
                    </exclusions>
                </option>
            </options>
        </parameter>
        <parameter name="C_V_ICR_MAN" type="uint32" default="3">
            <description>EO ICR Mode</description>
            <options>
                <option name="On" value="2" />
                <option name="Off" value="3" />
            </options>
        </parameter>
        <parameter name="C_V_ICR_THR" type="uint32" default="0" min="0" max="255" step ="1">
            <description>EO ICR Threshold</description>
        </parameter>

        <!-- Gimbal params -->
        <parameter name="GB_MODE" type="uint32" default="0">
            <description>Gimbal Mode</description>
            <options>
                <option name="OFF" value="0" />
                <option name="LOCK" value="1" />
                <option name="FOLLOW" value="2" />
                <option name="MAPPING" value="3" />
                <option name="RETURN HOME" value="4" />
            </options>
        </parameter>
        <parameter name="GB_FW_FLAG" type="uint32" default="0">
            <description>Control Flags</description>
            <options>
                <option name="Overwrite" value="0" />
                <option name="Forward" value="1" />
            </options>
        </parameter>

        <!-- LRF params -->
        <parameter name="LRF_MODE" type="uint32" default="0">
            <description>LRF Mode</description>
            <options>
                <option name="OFF" value="3" />
                <option name="1 Hz" value="0" />
                <option name="4 Hz" value="1" />
                <option name="10 Hz" value="2" />
            </options>
        </parameter>

    </parameters>
</mavlinkcamera>
