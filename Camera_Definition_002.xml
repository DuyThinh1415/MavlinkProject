                <option name="Manual" value="3">
                    <exclusions>
                        <exclude>C_V_ICR_THR</exclude>
                    </exclusions>
                </option>
            </options>
        </parameter>
        <parameter name="C_V_ICR_MAN" type="uint32" default="3">
            <description>ICR Mode</description>
            <options>
                <option name="On" value="2" />
                <option name="Off" value="3" />
            </options>
        </parameter>
        <parameter name="C_V_ICR_THR" type="uint32" default="0" min="0" max="255" step ="1">
