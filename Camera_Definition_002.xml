            <options>
                <option name="F2.0" value="25" />
                <option name="F2.2" value="24" />
                <option name="F2.4" value="23" />
                <option name="F2.6" value="22" />
                <option name="F2.8" value="21" />
                <option name="F3.1" value="20" />
                <option name="F3.4" value="19" />
                <option name="F4.0" value="17"/>
                <option name="F5.2" value="14" />
                <option name="F6.8" value="11" />
                <option name="F7.3" value="10" />
                <option name="F8.7" value="8" />
                <option name="F9.6" value="7" />
                <option name="F10" value="6" />
                <option name="F11" value="5" />
            </options>
        </parameter>

        <parameter name="C_V_IrP" type="uint32" default="0" min="0" max="25" step ="1">
            <description>Aperture Value</description>
        </parameter>

        <parameter name="C_V_BrP" type="uint32" default="0" min="0" max="41" step ="1">
            <description>Bright Value</description>
        </parameter>
        
        <parameter name="C_V_WB" type="uint32" default="0">
            <description>WB Mode</description>
            <options>
                <option name="Auto" value="0" />
                <option name="Indoor" value="1" />
                <option name="Outdoor" value="2" />
                <option name="One Push WB" value="3" />
                <option name="ATW" value="4" />
                <option name="Manual" value="5" />
            </options>
        </parameter>

        <parameter name="C_V_ZM_MODE" type="uint32" default="0">
            <description>Zoom Mode</description>
            <options>
                <option name="Combine" value="0">
                    <exclusions>
                        <exclude>C_V_ZM_SR</exclude>
                        <exclude>C_V_ZM_SR_LV</exclude>
                    </exclusions>
                </option>
                <option name="Super Resolution" value="2">
                    <exclusions>
                        <exclude>C_V_ZM_CB</exclude>
                        <exclude>C_V_ZM_CB_LV</exclude>
                    </exclusions>
