            <description>Zoom Level</description>
            <options>
                <option name="1x" value="0" />
                <option name="10x" value="1" />
                <option name="20x" value="2" />
                <option name="40x" value="3" />
                <option name="80x" value="4" />
                <option name="120x" value="5" />
                <option name="240x" value="6" />
            </options>
        </parameter>

        <parameter name="C_V_FM" type="uint32" default="0">
            <description>EO Focus Mode</description>
            <options>
                <option name="Manual" value="0">
                    <exclusions>
                        <exclude>C_V_FA</exclude>
                    </exclusions>
                </option>
                <option name="Auto Focus" value="1">
                    <exclusions>
                        <exclude>C_V_FV</exclude>
                    </exclusions>
                </option>
                <option name="Auto Focus OnePush" value="2">
                    <exclusions>
                        <exclude>C_V_FV</exclude>
                        <exclude>C_V_FA</exclude>
                    </exclusions>
                </option>
            </options>
        </parameter>
        <parameter name="C_V_FV" type="uint32" default="0" min="0" max="61440" step ="1">
            <description>EO Manual Focus Value</description>
        </parameter>
        <parameter name="C_V_FA" type="uint32" default="0">
            <description>EO Auto Focus Mode</description>
            <options>
                <option name="Normal" value="0" />
